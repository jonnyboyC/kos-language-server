{
  "name": "kos-language-server",
  "description": "A Language server for Kerbal Operating System. A mod for Kerbal Space Program.",
  "version": "1.1.2",
  "main": "out/src/server.js",
  "types": "out/src/server.d.ts",
  "bin": {
    "kls": "./bin/kos"
  },
  "files": [
    "dist/**/*",
    "out/**/*",
    "bin"
  ],
  "author": "John Chabot",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonnyboyC/kos-language-server"
  },
  "dependencies": {
    "commander": "^4.1.0",
    "denque": "^1.4.1",
    "jsonc-parser": "^2.2.0",
    "ts-set-utils": "^0.2.0",
    "vscode-languageserver": "^6.0.0",
    "vscode-languageserver-textdocument": "^1.0.0",
    "vscode-uri": "^2.1.1"
  },
  "keywords": [
    "Kerbal Operating System",
    "kos",
    "Kerbal Space Program",
    "ksp",
    "language-server"
  ],
  "scripts": {
    "bench": "zakzak -c ./zakzak.config.json",
    "build": "tsc -p ./tsconfig.json",
    "bundle": "webpack --config ./webpack.config.js --mode production",
    "test": "jest --config jest.config.js --verbose",
    "prepack": "npm run build && npm run bundle && npm test"
  },
  "devDependencies": {
    "@dynatrace/zakzak": "^0.6.0",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.8",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "jstat": "^1.9.2",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
