{
  "name": "kos-language-server",
  "description": "A Language server for Kerbal Operating System. A mod for Kerbal Space Program.",
  "version": "1.1.4",
  "main": "out/src/server.js",
  "types": "out/src/server.d.ts",
  "bin": {
    "kls": "./bin/kos"
  },
  "files": [
    "dist/**/*",
    "out/**/*",
    "bin"
  ],
  "author": "John Chabot",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonnyboyC/kos-language-server"
  },
  "dependencies": {
    "commander": "^5.1.0",
    "denque": "^1.4.1",
    "jsonc-parser": "^2.3.0",
    "ts-set-utils": "^0.2.0",
    "vscode-languageserver": "^6.1.1",
    "vscode-languageserver-textdocument": "^1.0.1",
    "vscode-uri": "^2.1.2"
  },
  "keywords": [
    "Kerbal Operating System",
    "kos",
    "Kerbal Space Program",
    "ksp",
    "language-server"
  ],
  "scripts": {
    "bench": "zakzak -c ./zakzak.config.json",
    "build": "tsc -p ./tsconfig.json",
    "bundle": "webpack --config ./webpack.config.js --mode production",
    "test": "jest --config jest.config.js --verbose",
    "prepack": "npm run build && npm run bundle && npm test"
  },
  "devDependencies": {
    "@dynatrace/zakzak": "^0.6.3",
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.14",
    "jest": "^25.5.4",
    "jest-junit": "^10.0.0",
    "jstat": "^1.9.3",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.2",
    "tslint": "^6.1.2",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
